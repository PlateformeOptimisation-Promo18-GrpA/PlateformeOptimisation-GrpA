<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DemarageController">
   <center>
      <VBox alignment="CENTER" maxHeight="500.0" maxWidth="500.0" minHeight="500.0" minWidth="500.0" prefHeight="500.0" prefWidth="500.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Bienvenu dans">
               <font>
                  <Font name="Bodoni MT" size="40.0" />
               </font>
            </Label>
            <Label text="l'optimisateur de problèmes">
               <font>
                  <Font name="Bodoni MT" size="40.0" />
               </font>
            </Label>
            <Button fx:id="boutonDemarrage" mnemonicParsing="false" onAction="#boutonDemarrageClick" text="Démarrer">
               <font>
                  <Font name="Book Antiqua Bold" size="36.0" />
               </font>
               <VBox.margin>
                  <Insets top="50.0" />
